<query id="insertOffer" action="insert">
    <tables>
        <table name="project_offer" />
    </tables>
    <columns>
        <column name="offer_srl" var="offer_srl" notnull="notnull" />
        <column name="site_srl" var="site_srl" notnull="notnull" />
        <column name="title" var="title" notnull="notnull" />
        <column name="content" var="content" notnull="notnull" />
        <column name="regdate" default="curdate()" />
        <column name="list_order" var="list_order" notnull="notnull" />
    </columns>
</query>
