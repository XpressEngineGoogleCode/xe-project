<query id="deleteNews" action="delete">
    <tables>
        <table name="project_news" />
    </tables>
    <conditions>
        <condition operation="equal" column="site_srl" var="site_srl" filter="number" notnull="notnull" />
        <condition operation="equal" column="news_srl" var="news_srl" filter="number" pipe="and" />
    </conditions>
</query>
